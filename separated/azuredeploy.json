{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "baseUrl": {"type": "string"},
    "baseBlob": {"type": "string"}
  },
  "variables": { },
  "resources": [
    {
      "name": "storageBlob",
      "type": "Microsoft.Resources/deployments",
      "apiVersion": "2020-10-01",
      "properties": {
        "mode": "Incremental",
        "templateLink": {
          "uri": "[concat(parameters('baseUrl'), parameters('baseBlob'), 'template.json')]"
        },
        "parametersLink": {
          "uri": "[concat(parameters('baseUrl'), parameters('baseBlob'), 'template.parameters.json')]"
        }
      }
    }
  ],
  "outputs": {  }
}