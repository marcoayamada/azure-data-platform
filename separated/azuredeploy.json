{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {

  },
  "functions": [],
  "variables": {
    "templateBaseUrl":"https://raw.githubusercontent.com/marcoayamada/azure-data-platform/develop/separated/storage/blob.json",
    
    "blobTemplateUrl": "https://raw.githubusercontent.com/marcoayamada/azure-data-platform/develop/separated/storage/blob.json",
    "blobParametersUrl": "https://raw.githubusercontent.com/marcoayamada/azure-data-platform/develop/separated/storage/blob.parameters.json"
  },
  "resources": [
    {
      "name": "siblingLinked",
      "type": "Microsoft.Resources/deployments",
      "apiVersion": "2020-10-01",
      "properties": {
        "mode": "Incremental",
        "templateLink": {
          "uri": "[variables('blobTemplateUrl')]"
        },
        "parametersLink": {
        "uri": "[variables('blobParametersUrl')]"
      }
      }
    }
  ],
  "outputs": {
    "fromSibling": {
      "type": "string",
      "value": "[reference('siblingLinked').outputs.fromChild.value]"
    }
  }
}